{
    "env": {
        "es6": true,
        "browser": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "mocha"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-unused-vars": 1
    },
    "settings": {}
}