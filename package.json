{
  "name": "eh",
  "version": "1.0.0",
  "description": "Simple tiny event manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Foursk-main/eh.git"
  },
  "keywords": [
    "Event",
    "eh"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && mocha --require @babel/register --require @babel/polyfill tests/**/*.test.js",
    "bench": "babel-node tests/benchmark.js",
    "tscw": "tsc --watch",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format && npm run lint && npm test"
