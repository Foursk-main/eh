{
  "name": "@foursk/eh",
  "version": "1.0.0",
  "description": "Simple tiny event manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Foursk-main/eh.git"
  },
  "keywords": [
    "Event",
    "eh",
    "foursk"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && npm run build && mocha --require @babel/register --require @babel/polyfill tests/**/*.test.js",
    "bench": "babel-node tests/benchmark.js",
    "tscw": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json && eslint -c .eslintrc.json examples tests",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format && npm run lint && npm test"
  },
  "author": "foursk",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^5.2.0",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {},
  "files": [
    "/dist/**/*"
  ]
}
